<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.halfmoon.cloudmanager.dao.sql.user.icon.impl.IconDao">
	
	<insert id="add"
			parameterType="com.halfmoon.cloudmanager.model.user.icon.Icon"
			useGeneratedKeys="true"
			keyProperty="id">
			
		INSERT INTO icon (url, user_id) VALUES (#{url}, #{user_id})
			
	</insert>
	
</mapper>